var _logging = require("../../logging");
describe('logging/index.js (barrel export)', function () {
  test('should export logger', function () {
    expect(_logging.logger).toBeDefined();
    expect(typeof _logging.logger.debug).toBe('function');
    expect(typeof _logging.logger.info).toBe('function');
    expect(typeof _logging.logger.warn).toBe('function');
    expect(typeof _logging.logger.error).toBe('function');
    expect(typeof _logging.logger.fatal).toBe('function');
  });
  test('should export all log levels', function () {
    expect(_logging.DEBUG).toBe('debug');
    expect(_logging.INFO).toBe('info');
    expect(_logging.WARN).toBe('warn');
    expect(_logging.ERROR).toBe('error');
    expect(_logging.FATAL).toBe('fatal');
  });
  test('should have correct barrel export structure', function () {
    var expectedExports = ['logger', 'DEBUG', 'INFO', 'WARN', 'ERROR', 'FATAL'];
    expectedExports.forEach(function (exportName) {
      expect({
        logger: _logging.logger,
        DEBUG: _logging.DEBUG,
        INFO: _logging.INFO,
        WARN: _logging.WARN,
        ERROR: _logging.ERROR,
        FATAL: _logging.FATAL
      }).toHaveProperty(exportName);
    });
  });
});
