    mockBootstrapApp.mockRejectedValue(bootstrapError);
    var _render5 = (0, _reactNative2.render)((0, _jsxRuntime.jsx)(_layout.default, {})),
      UNSAFE_root = _render5.UNSAFE_root;
    yield (0, _reactNative2.waitFor)(function () {
      expect(mockBootstrapApp).toHaveBeenCalled();
