describe('app/_layout.jsx - ErrorBoundary Integration', function () {
  beforeEach(function () {
    jest.clearAllMocks();
    mockBootstrapApp.mockResolvedValue(undefined);
    mockSlotRenderer = function mockSlotRenderer() {
      return _react.default.createElement(_reactNative.Text, null, 'Test Content');
    };
  });
  test('should render normally without errors', (0, _asyncToGenerator2.default)(function* () {
    var _render = (0, _reactNative2.render)((0, _jsxRuntime.jsx)(_layout.default, {})),
      getByText = _render.getByText;
    yield (0, _reactNative2.waitFor)(function () {
      expect(getByText('Test Content')).toBeTruthy();
    }, {
      timeout: 3000
    });
    expect(mockBootstrapApp).toHaveBeenCalled();
  }));
  test('should display fallback UI when child component throws error', (0, _asyncToGenerator2.default)(function* () {
    var ThrowError = function ThrowError() {
      throw new Error('Test error');
    };
    mockSlotRenderer = function mockSlotRenderer() {
      return _react.default.createElement(ThrowError);
    };
    var _render2 = (0, _reactNative2.render)((0, _jsxRuntime.jsx)(_layout.default, {})),
      getByText = _render2.getByText;
    yield (0, _reactNative2.waitFor)(function () {
      expect(getByText('Something went wrong')).toBeTruthy();
    }, {
      timeout: 5000
    });
    expect(_error.handleError).toHaveBeenCalled();
    expect(_logging.logger.error).toHaveBeenCalled();
  }));
  test('should not expose raw error details in fallback UI', (0, _asyncToGenerator2.default)(function* () {
    var ThrowSensitiveError = function ThrowSensitiveError() {
      var error = new Error('Sensitive error: API_KEY=secret123');
      error.stack = 'Error stack trace with sensitive data';
      throw error;
    };
    mockSlotRenderer = function mockSlotRenderer() {
      return _react.default.createElement(ThrowSensitiveError);
    };
    var _render3 = (0, _reactNative2.render)((0, _jsxRuntime.jsx)(_layout.default, {})),
      getByText = _render3.getByText,
      queryByText = _render3.queryByText;
    yield (0, _reactNative2.waitFor)(function () {
      expect(getByText('Something went wrong')).toBeTruthy();
    }, {
      timeout: 5000
    });
    expect(queryByText(/API_KEY/)).toBeNull();
    expect(queryByText(/secret123/)).toBeNull();
    expect(queryByText(/stack trace/)).toBeNull();
    expect(queryByText(/Sensitive error/)).toBeNull();
    expect(getByText('An unexpected error occurred')).toBeTruthy();
  }));
  test('should handle multiple children without errors', (0, _asyncToGenerator2.default)(function* () {
    mockSlotRenderer = function mockSlotRenderer() {
      return _react.default.createElement(_react.default.Fragment, null, _react.default.createElement(_reactNative.Text, null, 'First Child'), _react.default.createElement(_reactNative.Text, null, 'Second Child'), _react.default.createElement(_reactNative.Text, null, 'Third Child'));
    };
    var _render4 = (0, _reactNative2.render)((0, _jsxRuntime.jsx)(_layout.default, {})),
      getByText = _render4.getByText;
    yield (0, _reactNative2.waitFor)(function () {
      expect(getByText('First Child')).toBeTruthy();
      expect(getByText('Second Child')).toBeTruthy();
      expect(getByText('Third Child')).toBeTruthy();
    }, {
      timeout: 3000
    });
  }));
  test('should handle empty state without error', (0, _asyncToGenerator2.default)(function* () {
    mockSlotRenderer = function mockSlotRenderer() {
      return null;
    };
