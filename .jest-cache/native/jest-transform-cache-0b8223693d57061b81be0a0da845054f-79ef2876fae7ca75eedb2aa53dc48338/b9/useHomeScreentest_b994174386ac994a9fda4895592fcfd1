var React = require('react');
var TestRenderer = require('react-test-renderer');
var useHomeScreen = require("../../../../../platform/screens/main/HomeScreen/useHomeScreen").default;
var act = TestRenderer.act;
var renderHook = function renderHook(hook) {
  var _ref = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : {},
    initialProps = _ref.initialProps;
  var result = {};
  var renderer;
  var HookHarness = function HookHarness(_ref2) {
    var hookProps = _ref2.hookProps;
    var hookResult = hook(hookProps);
    Object.assign(result, hookResult);
    return null;
  };
  act(function () {
    renderer = TestRenderer.create(React.createElement(HookHarness, {
      hookProps: initialProps
    }));
  });
  return {
    result: {
      current: result
    },
    rerender: function rerender(newProps) {
      act(function () {
        renderer.update(React.createElement(HookHarness, {
          hookProps: newProps
        }));
      });
    },
    unmount: function unmount() {
      act(function () {
        renderer.unmount();
      });
    }
  };
};
describe('useHomeScreen Hook', function () {
  it('should return an object', function () {
    var _renderHook = renderHook(function () {
        return useHomeScreen();
      }),
      result = _renderHook.result;
    expect(result.current).toBeDefined();
    expect(typeof result.current).toBe('object');
  });
  it('should not throw when called', function () {
    expect(function () {
      renderHook(function () {
        return useHomeScreen();
      });
    }).not.toThrow();
  });
  it('should return consistent structure', function () {
    var _renderHook2 = renderHook(function () {
        return useHomeScreen();
      }),
      result = _renderHook2.result,
      rerender = _renderHook2.rerender;
    var firstResult = result.current;
    rerender();
    var secondResult = result.current;
    expect(Object.keys(firstResult)).toEqual(Object.keys(secondResult));
  });
});
