  }));
  it('clears timers on unmount (no post-unmount state update)', (0, _asyncToGenerator2.default)(function* () {
    var consoleError = jest.spyOn(console, 'error').mockImplementation(function () {});
    var results = [];
    var _render3 = (0, _reactNative.render)((0, _jsxRuntime.jsx)(TestComponent, {
