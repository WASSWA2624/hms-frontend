  return {
    queueRequestIfOffline: jest.fn()
  };
});
var _department = require("../../../features/department");
var _department2 = require("../../../features/department/department.api");
var _request = require("../../../offline/request");
var _crudUsecaseRunner = require("../../helpers/crud-usecase-runner");
function _getJestObj() {
  var _require = require("@jest/globals"),
    jest = _require.jest;
  _getJestObj = function _getJestObj() {
    return jest;
  };
  return jest;
}
describe('department.usecase', function () {
  beforeEach(function () {
    _department2.departmentApi.list.mockResolvedValue({
      data: [{
        id: '1'
      }]
    });
    _department2.departmentApi.get.mockResolvedValue({
      data: {
        id: '1'
      }
    });
    _department2.departmentApi.create.mockResolvedValue({
      data: {
        id: '1'
      }
    });
    _department2.departmentApi.update.mockResolvedValue({
      data: {
        id: '1'
      }
    });
    _department2.departmentApi.remove.mockResolvedValue({
      data: {
        id: '1'
      }
    });
    _department2.getDepartmentUnitsApi.mockResolvedValue({
      data: [{
        id: '1'
      }]
    });
  });
  (0, _crudUsecaseRunner.runCrudUsecaseTests)({
    list: _department.listDepartments,
    get: _department.getDepartment,
    create: _department.createDepartment,
    update: _department.updateDepartment,
    remove: _department.deleteDepartment,
    extraActions: [{
