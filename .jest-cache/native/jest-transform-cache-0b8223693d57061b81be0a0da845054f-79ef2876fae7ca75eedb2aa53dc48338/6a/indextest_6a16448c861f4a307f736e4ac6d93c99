var getCjsOrEsmDefault = function getCjsOrEsmDefault(mod) {
  var _mod$default;
  return (_mod$default = mod == null ? void 0 : mod.default) != null ? _mod$default : mod;
};
describe('Theme Provider & Resolver (Step 3.11)', function () {
  it('should render theme provider', function () {
    var themeIndex = require("../../theme");
    var api = getCjsOrEsmDefault(themeIndex);
    expect(api.ThemeProvider).toBeDefined();
    expect(typeof api.ThemeProvider).toBe('function');
  });
  it('should have theme resolver (getTheme function)', function () {
    var themeIndex = require("../../theme");
    var api = getCjsOrEsmDefault(themeIndex);
    expect(api.getTheme).toBeDefined();
    expect(typeof api.getTheme).toBe('function');
  });
  it('should switch themes correctly', function () {
    var themeIndex = require("../../theme");
    var api = getCjsOrEsmDefault(themeIndex);
    var lightTheme = getCjsOrEsmDefault(require("../../theme/light.theme"));
    var darkTheme = getCjsOrEsmDefault(require("../../theme/dark.theme"));
    var highContrastTheme = getCjsOrEsmDefault(require("../../theme/high-contrast.theme"));
    expect(api.getTheme('light')).toEqual(lightTheme);
    expect(api.getTheme('dark')).toEqual(darkTheme);
    expect(api.getTheme('high-contrast')).toEqual(highContrastTheme);
    expect(api.getTheme()).toEqual(lightTheme);
  });
  it('should export all themes', function () {
    var themeIndex = require("../../theme");
    var api = getCjsOrEsmDefault(themeIndex);
    expect(api.lightTheme).toBeDefined();
    expect(api.darkTheme).toBeDefined();
    expect(api.highContrastTheme).toBeDefined();
  });
});
