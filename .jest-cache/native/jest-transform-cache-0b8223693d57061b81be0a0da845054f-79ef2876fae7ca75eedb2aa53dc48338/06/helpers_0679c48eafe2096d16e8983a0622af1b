var clamp = exports.clamp = function clamp(value, min, max) {
  if (typeof value !== 'number') return min;
  if (typeof min !== 'number') return value;
  if (typeof max !== 'number') return value;
  return Math.min(Math.max(value, min), max);
};
var normalizeWhitespace = exports.normalizeWhitespace = function normalizeWhitespace(value) {
  if (typeof value !== 'string') return '';
  return value.trim().replace(/\s+/g, ' ');
};
var safeJsonParse = exports.safeJsonParse = function safeJsonParse(value) {
  var fallback = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : null;
  if (typeof value !== 'string') return fallback;
  try {
    return JSON.parse(value);
  } catch (_unused) {
    return fallback;
  }
};
