var _utils = require("../../utils");
describe('Validator Utilities', function () {
  describe('isValidEmail', function () {
    test('should return true for valid email', function () {
      expect((0, _utils.isValidEmail)('test@example.com')).toBe(true);
      expect((0, _utils.isValidEmail)('user.name@domain.co.uk')).toBe(true);
      expect((0, _utils.isValidEmail)('user+tag@example.com')).toBe(true);
    });
    test('should return false for invalid email', function () {
      expect((0, _utils.isValidEmail)('invalid')).toBe(false);
      expect((0, _utils.isValidEmail)('invalid@')).toBe(false);
      expect((0, _utils.isValidEmail)('@example.com')).toBe(false);
      expect((0, _utils.isValidEmail)('invalid@example')).toBe(false);
      expect((0, _utils.isValidEmail)('invalid..email@example.com')).toBe(false);
    });
    test('should handle whitespace by trimming', function () {
      expect((0, _utils.isValidEmail)('  test@example.com  ')).toBe(true);
      expect((0, _utils.isValidEmail)(' test@example.com')).toBe(true);
      expect((0, _utils.isValidEmail)('test@example.com ')).toBe(true);
    });
    test('should return false for null', function () {
      expect((0, _utils.isValidEmail)(null)).toBe(false);
    });
    test('should return false for undefined', function () {
      expect((0, _utils.isValidEmail)(undefined)).toBe(false);
    });
    test('should return false for non-string', function () {
      expect((0, _utils.isValidEmail)(123)).toBe(false);
      expect((0, _utils.isValidEmail)({})).toBe(false);
      expect((0, _utils.isValidEmail)([])).toBe(false);
    });
    test('should return false for empty string', function () {
      expect((0, _utils.isValidEmail)('')).toBe(false);
      expect((0, _utils.isValidEmail)('   ')).toBe(false);
    });
  });
  describe('isValidUrl', function () {
    test('should return true for valid URLs', function () {
      expect((0, _utils.isValidUrl)('https://example.com')).toBe(true);
      expect((0, _utils.isValidUrl)('http://example.com')).toBe(true);
      expect((0, _utils.isValidUrl)('https://www.example.com/path')).toBe(true);
      expect((0, _utils.isValidUrl)('https://example.com:8080/path?query=value')).toBe(true);
    });
    test('should return false for invalid URLs', function () {
      expect((0, _utils.isValidUrl)('not-a-url')).toBe(false);
      expect((0, _utils.isValidUrl)('example.com')).toBe(false);
      expect((0, _utils.isValidUrl)('ftp://example.com')).toBe(false);
    });
    test('should return false for null', function () {
      expect((0, _utils.isValidUrl)(null)).toBe(false);
    });
    test('should return false for undefined', function () {
      expect((0, _utils.isValidUrl)(undefined)).toBe(false);
    });
    test('should return false for non-string', function () {
      expect((0, _utils.isValidUrl)(123)).toBe(false);
      expect((0, _utils.isValidUrl)({})).toBe(false);
      expect((0, _utils.isValidUrl)([])).toBe(false);
    });
    test('should return false for empty string', function () {
      expect((0, _utils.isValidUrl)('')).toBe(false);
    });
  });
});
