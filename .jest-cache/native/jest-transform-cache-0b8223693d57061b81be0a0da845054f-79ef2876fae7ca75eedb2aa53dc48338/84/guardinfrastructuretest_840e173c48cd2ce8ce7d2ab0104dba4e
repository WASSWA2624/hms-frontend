var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault").default;
var _fs = _interopRequireDefault(require("fs"));
var _path = _interopRequireDefault(require("path"));
describe('Step 7.11: Guard Infrastructure Folder Structure', function () {
  var navigationDir = _path.default.join(__dirname, '../../navigation');
  var guardsDir = _path.default.join(navigationDir, 'guards');
  var indexFile = _path.default.join(guardsDir, 'index.js');
  test('should have guards directory', function () {
    expect(_fs.default.existsSync(guardsDir)).toBe(true);
  });
  test('should have barrel export file', function () {
    expect(_fs.default.existsSync(indexFile)).toBe(true);
  });
  test('should be located in navigation directory', function () {
    expect(_fs.default.existsSync(navigationDir)).toBe(true);
    expect(_fs.default.existsSync(guardsDir)).toBe(true);
    var stats = _fs.default.statSync(guardsDir);
    expect(stats.isDirectory()).toBe(true);
  });
  test('should have index.js as a file', function () {
    var stats = _fs.default.statSync(indexFile);
    expect(stats.isFile()).toBe(true);
  });
  test('should allow guard files to exist in guards directory', function () {
    var authGuardFile = _path.default.join(guardsDir, 'auth.guard.js');
    var roleGuardFile = _path.default.join(guardsDir, 'role.guard.js');
    expect(_fs.default.existsSync(guardsDir)).toBe(true);
    if (_fs.default.existsSync(authGuardFile)) {
      var stats = _fs.default.statSync(authGuardFile);
      expect(stats.isFile()).toBe(true);
    }
    if (_fs.default.existsSync(roleGuardFile)) {
      var _stats = _fs.default.statSync(roleGuardFile);
      expect(_stats.isFile()).toBe(true);
    }
  });
  test('should export guards from barrel export file', function () {
    var indexModule = require(indexFile);
    expect(indexModule).toBeDefined();
    expect(typeof indexModule).toBe('object');
  });
});
