_getJestObj().mock("../../i18n", function () {
  var actualI18n = jest.requireActual("../../i18n");
  var React = require('react');
  var mockCreateI18n = jest.fn(function () {
    return {
      tSync: jest.fn(function (key, params) {
        if (key === 'common.save') return 'Save';
        if (key === 'common.cancel') return 'Cancel';
        if (key === 'common.loading') return 'Loading...';
        if (key === 'greeting.hello' && params != null && params.name) {
          return `Hello, ${params.name}`;
        }
        return key;
      }),
      getCurrentLocale: jest.fn(function () {
        return Promise.resolve('en');
      }),
      setLocale: jest.fn(function () {
        return Promise.resolve();
      }),
      supportedLocales: ['en']
    };
  });
  return Object.assign({}, actualI18n, {
    createI18n: mockCreateI18n,
    getDeviceLocale: jest.fn(function () {
      return 'en';
    }),
    tSync: jest.fn(function (key, params) {
      if (key === 'common.save') return 'Save';
      if (key === 'common.cancel') return 'Cancel';
      return key;
    })
  });
}, {
  virtual: true
});
