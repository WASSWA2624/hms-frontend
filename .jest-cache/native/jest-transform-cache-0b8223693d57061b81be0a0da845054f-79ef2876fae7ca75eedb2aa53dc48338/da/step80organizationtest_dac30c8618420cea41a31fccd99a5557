c7d00bf12babf1c7e8a33412210ba904
var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault").default;
var _fs = _interopRequireDefault(require("fs"));
var _path = _interopRequireDefault(require("path"));
describe('Step 8.0: App Folder Organization', function () {
  var srcPath = _path.default.join(process.cwd(), 'src');
  var appPath = _path.default.join(srcPath, 'app');
  var platformLayoutsPath = _path.default.join(srcPath, 'platform', 'layouts');
  var platformCommonPath = _path.default.join(platformLayoutsPath, 'common');
  var platformRouteLayoutsPath = _path.default.join(platformLayoutsPath, 'RouteLayouts');
  describe('Layout Helpers Organization', function () {
    it('should have ThemeProviderWrapper in platform/layouts/common/', function () {
      var themeProviderPath = _path.default.join(platformCommonPath, 'ThemeProviderWrapper');
      expect(_fs.default.existsSync(themeProviderPath)).toBe(true);
      var componentFile = _path.default.join(themeProviderPath, 'ThemeProviderWrapper.jsx');
      var indexFile = _path.default.join(themeProviderPath, 'index.js');
      expect(_fs.default.existsSync(componentFile)).toBe(true);
      expect(_fs.default.existsSync(indexFile)).toBe(true);
    });
    it('should NOT have ThemeProviderWrapper in app/', function () {
      var appThemeProviderPath = _path.default.join(appPath, 'ThemeProviderWrapper');
      expect(_fs.default.existsSync(appThemeProviderPath)).toBe(false);
    });
    it('should import ThemeProviderWrapper from platform in root layout', function () {
      var rootLayoutPath = _path.default.join(appPath, '_layout.jsx');
      var rootLayoutContent = _fs.default.readFileSync(rootLayoutPath, 'utf8');
      expect(rootLayoutContent).toMatch(/@platform\/layouts\/common\/ThemeProviderWrapper/);
      expect(rootLayoutContent).not.toMatch(/\.\/ThemeProviderWrapper/);
      expect(rootLayoutContent).not.toMatch(/\.\.\/ThemeProviderWrapper/);
    });
  });
  describe('Layout Styles Organization', function () {
    it('should have RootLayoutStyles in platform/layouts/common/', function () {
      var rootLayoutStylesPath = _path.default.join(platformCommonPath, 'RootLayoutStyles');
      expect(_fs.default.existsSync(rootLayoutStylesPath)).toBe(true);
    });
    it('should have platform-specific style files for RootLayoutStyles', function () {
      var rootLayoutStylesPath = _path.default.join(platformCommonPath, 'RootLayoutStyles');
      var androidStyles = _path.default.join(rootLayoutStylesPath, 'RootLayoutStyles.android.styles.jsx');
      var iosStyles = _path.default.join(rootLayoutStylesPath, 'RootLayoutStyles.ios.styles.jsx');
      var webStyles = _path.default.join(rootLayoutStylesPath, 'RootLayoutStyles.web.styles.jsx');
      var indexFile = _path.default.join(rootLayoutStylesPath, 'index.js');
      expect(_fs.default.existsSync(androidStyles)).toBe(true);
      expect(_fs.default.existsSync(iosStyles)).toBe(true);
      expect(_fs.default.existsSync(webStyles)).toBe(true);
      expect(_fs.default.existsSync(indexFile)).toBe(true);
    });
    it('should NOT have RootLayoutStyles in app/', function () {
      var appRootLayoutStylesPath = _path.default.join(appPath, 'RootLayoutStyles');
      expect(_fs.default.existsSync(appRootLayoutStylesPath)).toBe(false);
    });
    it('should use correct styled-components entrypoints in RootLayoutStyles', function () {
      var rootLayoutStylesPath = _path.default.join(platformCommonPath, 'RootLayoutStyles');
      var androidStyles = _fs.default.readFileSync(_path.default.join(rootLayoutStylesPath, 'RootLayoutStyles.android.styles.jsx'), 'utf8');
      var iosStyles = _fs.default.readFileSync(_path.default.join(rootLayoutStylesPath, 'RootLayoutStyles.ios.styles.jsx'), 'utf8');
      var webStyles = _fs.default.readFileSync(_path.default.join(rootLayoutStylesPath, 'RootLayoutStyles.web.styles.jsx'), 'utf8');
      expect(androidStyles).toMatch(/from ['"]styled-components\/native['"]/);
      expect(iosStyles).toMatch(/from ['"]styled-components\/native['"]/);
      expect(webStyles).toMatch(/from ['"]styled-components['"]/);
      expect(webStyles).not.toMatch(/from ['"]styled-components\/native['"]/);
    });
    it('should NOT have default exports in RootLayoutStyles style files', function () {
      var rootLayoutStylesPath = _path.default.join(platformCommonPath, 'RootLayoutStyles');
      var androidStyles = _fs.default.readFileSync(_path.default.join(rootLayoutStylesPath, 'RootLayoutStyles.android.styles.jsx'), 'utf8');
      var iosStyles = _fs.default.readFileSync(_path.default.join(rootLayoutStylesPath, 'RootLayoutStyles.ios.styles.jsx'), 'utf8');
      var webStyles = _fs.default.readFileSync(_path.default.join(rootLayoutStylesPath, 'RootLayoutStyles.web.styles.jsx'), 'utf8');
      expect(androidStyles).not.toMatch(/export default/);
      expect(iosStyles).not.toMatch(/export default/);
      expect(webStyles).not.toMatch(/export default/);
    });
    it('should import RootLayoutStyles from platform in root layout', function () {
      var rootLayoutPath = _path.default.join(appPath, '_layout.jsx');
      var rootLayoutContent = _fs.default.readFileSync(rootLayoutPath, 'utf8');
      expect(rootLayoutContent).toMatch(/@platform\/layouts\/common\/RootLayoutStyles/);
      expect(rootLayoutContent).not.toMatch(/\.\/RootLayoutStyles/);
      expect(rootLayoutContent).not.toMatch(/\.\.\/RootLayoutStyles/);
    });
  });
  describe('Route File Organization', function () {
    it('should have lightweight route files', function () {
      var routeFiles = ['index.jsx', '+not-found.jsx', '_error.jsx'];
      routeFiles.forEach(function (file) {
        var filePath = _path.default.join(appPath, file);
        if (_fs.default.existsSync(filePath)) {
          var content = _fs.default.readFileSync(filePath, 'utf8');
          expect(content.length).toBeLessThan(2000);
        }
      });
    });
    it('should have route files that only import platform UI components', function () {
      var indexPath = _path.default.join(appPath, 'index.jsx');
      if (_fs.default.existsSync(indexPath)) {
        var content = _fs.default.readFileSync(indexPath, 'utf8');
        expect(content).toMatch(/@platform/);
      }
    });
  });
  describe('Route Group Layouts Organization', function () {
    it('should have route groups with parentheses', function () {
      var mainGroupPath = _path.default.join(appPath, '(main)');
      expect(_fs.default.existsSync(mainGroupPath)).toBe(true);
    });
    it('should have single _layout.jsx file per route group', function () {
      var mainLayoutPath = _path.default.join(appPath, '(main)', '_layout.jsx');
      expect(_fs.default.existsSync(mainLayoutPath)).toBe(true);
      var mainLayoutAndroid = _path.default.join(appPath, '(main)', '_layout.android.jsx');
      var mainLayoutIos = _path.default.join(appPath, '(main)', '_layout.ios.jsx');
      var mainLayoutWeb = _path.default.join(appPath, '(main)', '_layout.web.jsx');
      expect(_fs.default.existsSync(mainLayoutAndroid)).toBe(false);
      expect(_fs.default.existsSync(mainLayoutIos)).toBe(false);
      expect(_fs.default.existsSync(mainLayoutWeb)).toBe(false);
    });
    it('should have main route layout importing from platform', function () {
      var mainLayoutPath = _path.default.join(appPath, '(main)', '_layout.jsx');
      var mainLayoutContent = _fs.default.readFileSync(mainLayoutPath, 'utf8');
      expect(mainLayoutContent).toMatch(/@platform\/layouts/);
      expect(mainLayoutContent).toMatch(/export default/);
      expect(mainLayoutContent).not.toMatch(/useEffect/);
      expect(mainLayoutContent).not.toMatch(/useState/);
    });
    it('should have layout logic in platform/layouts/RouteLayouts/', function () {
      var mainRouteLayoutPath = _path.default.join(platformRouteLayoutsPath, 'MainRouteLayout');
      expect(_fs.default.existsSync(mainRouteLayoutPath)).toBe(true);
      var mainRouteLayoutAndroid = _path.default.join(mainRouteLayoutPath, 'MainRouteLayout.android.jsx');
      var mainRouteLayoutIos = _path.default.join(mainRouteLayoutPath, 'MainRouteLayout.ios.jsx');
      var mainRouteLayoutWeb = _path.default.join(mainRouteLayoutPath, 'MainRouteLayout.web.jsx');
      expect(_fs.default.existsSync(mainRouteLayoutAndroid)).toBe(true);
      expect(_fs.default.existsSync(mainRouteLayoutIos)).toBe(true);
      expect(_fs.default.existsSync(mainRouteLayoutWeb)).toBe(true);
    });
  });
  describe('Platform Layouts Barrel Export', function () {
    it('should export ThemeProviderWrapper from platform layouts barrel', function () {
      var layoutsIndexPath = _path.default.join(platformLayoutsPath, 'index.js');
      var layoutsIndexContent = _fs.default.readFileSync(layoutsIndexPath, 'utf8');
      expect(layoutsIndexContent).toMatch(/ThemeProviderWrapper/);
      expect(layoutsIndexContent).toMatch(/common\/ThemeProviderWrapper/);
    });
    it('should export RootLayoutStyles from platform layouts barrel', function () {
      var layoutsIndexPath = _path.default.join(platformLayoutsPath, 'index.js');
      var layoutsIndexContent = _fs.default.readFileSync(layoutsIndexPath, 'utf8');
      expect(layoutsIndexContent).toMatch(/RootLayoutStyles/);
      expect(layoutsIndexContent).toMatch(/common\/RootLayoutStyles/);
    });
  });
});
