var _hooks = require("../../hooks");
describe('@hooks barrel', function () {
  it('exports cross-cutting hooks as named exports', function () {
    expect(typeof _hooks.useTheme).toBe('function');
    expect(typeof _hooks.useNetwork).toBe('function');
    expect(typeof _hooks.useI18n).toBe('function');
    expect(typeof _hooks.useDebounce).toBe('function');
    expect(typeof _hooks.usePagination).toBe('function');
    expect(typeof _hooks.useAsyncState).toBe('function');
  });
});
