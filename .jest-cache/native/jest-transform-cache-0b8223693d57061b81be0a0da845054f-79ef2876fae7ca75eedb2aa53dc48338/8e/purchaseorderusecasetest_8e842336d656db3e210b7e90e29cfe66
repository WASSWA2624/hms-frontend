  return {
    queueRequestIfOffline: jest.fn()
  };
});
var _purchaseOrder = require("../../../features/purchase-order");
var _purchaseOrder2 = require("../../../features/purchase-order/purchase-order.api");
var _request = require("../../../offline/request");
var _crudUsecaseRunner = require("../../helpers/crud-usecase-runner");
function _getJestObj() {
  var _require = require("@jest/globals"),
    jest = _require.jest;
  _getJestObj = function _getJestObj() {
    return jest;
  };
  return jest;
}
describe('purchase-order.usecase', function () {
