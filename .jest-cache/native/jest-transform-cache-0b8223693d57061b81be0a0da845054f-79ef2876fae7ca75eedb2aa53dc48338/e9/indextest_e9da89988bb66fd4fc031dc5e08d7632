describe('Index Route (index.jsx)', function () {
  beforeEach(function () {
    return jest.clearAllMocks();
  });
  it('should redirect to /login when unauthenticated', function () {
    useAuthGuard.mockReturnValue({
      authenticated: false
    });
    var IndexRoute = require("../../app/index").default;
    renderWithTheme((0, _jsxRuntime.jsx)(IndexRoute, {}));
    expect(mockReplace).toHaveBeenCalledWith('/login');
  });
  it('should redirect to /home when authenticated', function () {
    useAuthGuard.mockReturnValue({
      authenticated: true
    });
    var IndexRoute = require("../../app/index").default;
    renderWithTheme((0, _jsxRuntime.jsx)(IndexRoute, {}));
    expect(mockReplace).toHaveBeenCalledWith('/home');
  });
  it('should use default export', function () {
    var mod = require("../../app/index");
    expect(mod.default).toBeDefined();
    expect(typeof mod.default).toBe('function');
  });
});
