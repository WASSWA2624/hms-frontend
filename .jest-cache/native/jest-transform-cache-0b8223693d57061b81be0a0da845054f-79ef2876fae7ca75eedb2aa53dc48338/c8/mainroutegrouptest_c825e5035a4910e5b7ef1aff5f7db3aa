var _interopRequireDefault = require("@babel/runtime/helpers/interopRequireDefault").default;
var _fs = _interopRequireDefault(require("fs"));
var _path = _interopRequireDefault(require("path"));
describe('Step 7.8: Main Route Group Folder Structure', function () {
  var appDir = _path.default.join(__dirname, '../../app');
  var mainGroupDir = _path.default.join(appDir, '(main)');
  test('should have main route group directory', function () {
    expect(_fs.default.existsSync(mainGroupDir)).toBe(true);
  });
  test('should follow route group naming convention', function () {
    var dirName = _path.default.basename(mainGroupDir);
    expect(dirName).toBe('(main)');
    expect(dirName.startsWith('(')).toBe(true);
    expect(dirName.endsWith(')')).toBe(true);
  });
  test('should be located in app directory', function () {
    expect(_fs.default.existsSync(appDir)).toBe(true);
    expect(_fs.default.existsSync(mainGroupDir)).toBe(true);
    var stats = _fs.default.statSync(mainGroupDir);
    expect(stats.isDirectory()).toBe(true);
  });
  test('should allow layout file to exist in main group', function () {
    var layoutFile = _path.default.join(mainGroupDir, '_layout.jsx');
    expect(_fs.default.existsSync(layoutFile)).toBe(true);
  });
});
