exports.formatNumber = exports.formatDate = exports.formatCurrency = void 0;
var formatDate = exports.formatDate = function formatDate(date) {
  var locale = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 'en-US';
  var options = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : {};
  if (!date) return '';
  var timeZone = options.timeZone || 'UTC';
  var parsed = new Date(date);
  if (Number.isNaN(parsed.getTime())) return '';
  return new Intl.DateTimeFormat(locale, {
    timeZone: timeZone
  }).format(parsed);
};
var formatCurrency = exports.formatCurrency = function formatCurrency(amount) {
  var currency = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 'USD';
  var locale = arguments.length > 2 && arguments[2] !== undefined ? arguments[2] : 'en-US';
  if (typeof amount !== 'number') return '';
  return new Intl.NumberFormat(locale, {
    style: 'currency',
    currency: currency
  }).format(amount);
};
var formatNumber = exports.formatNumber = function formatNumber(number) {
  var locale = arguments.length > 1 && arguments[1] !== undefined ? arguments[1] : 'en-US';
  if (typeof number !== 'number') return '';
  return new Intl.NumberFormat(locale).format(number);
};
