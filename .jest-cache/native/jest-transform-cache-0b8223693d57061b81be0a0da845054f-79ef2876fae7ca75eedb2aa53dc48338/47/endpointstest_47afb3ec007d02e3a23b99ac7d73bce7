var _endpoints = require("../../config/endpoints");
describe('endpoints.js', function () {
  test('should export endpoints object', function () {
    expect(_endpoints.endpoints).toBeDefined();
    expect(typeof _endpoints.endpoints).toBe('object');
  });
  test('should have AUTH endpoints', function () {
    expect(_endpoints.endpoints.AUTH).toBeDefined();
    expect(_endpoints.endpoints.AUTH.LOGIN).toBeDefined();
    expect(_endpoints.endpoints.AUTH.REGISTER).toBeDefined();
    expect(_endpoints.endpoints.AUTH.REFRESH).toBeDefined();
    expect(_endpoints.endpoints.AUTH.LOGOUT).toBeDefined();
  });
  test('should construct endpoints with base URL and version', function () {
    expect(_endpoints.endpoints.AUTH.LOGIN).toContain('/api/');
    expect(_endpoints.endpoints.AUTH.LOGIN).toContain('/auth/login');
  });
  test('should normalize base URL (remove trailing slashes)', function () {
    expect(_endpoints.endpoints.AUTH.LOGIN).not.toMatch(/\/\/api\//);
  });
  test('should have correct endpoint paths', function () {
    expect(_endpoints.endpoints.AUTH.LOGIN).toMatch(/\/auth\/login$/);
    expect(_endpoints.endpoints.AUTH.REGISTER).toMatch(/\/auth\/register$/);
  });
});
