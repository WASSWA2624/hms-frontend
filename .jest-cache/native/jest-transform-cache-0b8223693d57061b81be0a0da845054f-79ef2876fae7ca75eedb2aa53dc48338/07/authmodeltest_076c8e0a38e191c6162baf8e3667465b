var _auth = require("../../../features/auth");
describe('auth.model', function () {
  it('normalizes auth user and tokens', function () {
    expect((0, _auth.normalizeAuthUser)(null)).toBeNull();
    expect((0, _auth.normalizeAuthUser)({
      id: '1',
      name: 'User'
    })).toEqual({
      id: '1',
      name: 'User'
    });
    expect((0, _auth.normalizeAuthTokens)(null)).toBeNull();
    expect((0, _auth.normalizeAuthTokens)({
      access_token: 'a',
      refresh_token: 'b'
    })).toEqual({
      accessToken: 'a',
      refreshToken: 'b'
    });
  });
  it('normalizes auth response', function () {
    expect((0, _auth.normalizeAuthResponse)(null)).toEqual({
      user: null,
      tokens: null
    });
    expect((0, _auth.normalizeAuthResponse)({
      user: {
        id: '2'
      },
      tokens: {
        accessToken: 'a'
      }
    })).toEqual({
      user: {
        id: '2'
      },
      tokens: {
        accessToken: 'a',
        refreshToken: null
      }
    });
  });
});
