var _crudModel = require("../../utils/crudModel");
describe('crudModel', function () {
  it('normalizes entities and lists', function () {
    var model = (0, _crudModel.createCrudModel)();
    expect(model.normalize(null)).toBeNull();
    expect(model.normalize({
      id: 1,
      name: 'Test'
    })).toEqual({
      id: 1,
      name: 'Test'
    });
    expect(model.normalizeList(null)).toEqual([]);
    expect(model.normalizeList([{
      id: 1
    }, null, {
      id: 2
    }])).toEqual([{
      id: 1
    }, {
      id: 2
    }]);
  });
});
